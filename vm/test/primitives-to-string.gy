def check-eq(expected: String, actual: String) =
  if (expected != actual)
    throw Exception

def main =
  check-eq("unit", {}.to-string)
  check-eq("true", true.to-string)
  check-eq("false", false.to-string)
  check-eq("-12", -12i8.to-string)
  check-eq("-12", -12i16.to-string)
  check-eq("-12", -12i32.to-string)
  check-eq("-12", -12i64.to-string)
  check-eq("-1.2", -1.2f32.to-string)
  check-eq("-1.2", -1.2f64.to-string)
